{
  "name": "tree-sitter-fcp",
  "version": "1.0.0",
  "main": "grammar.js",
  "types": "bindings/node",
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "author": "",
  "license": "ISC",
  "gypfile": true,
  "description": "",
  "dependencies": {
    "node-gyp-build": "^4.8.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "^6.0.1"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "tree-sitter": [
    {
      "scope": "source.fcp",
      "injection-regex": "^fcp$",
      "file-types": [
        "fcp"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ]
}
