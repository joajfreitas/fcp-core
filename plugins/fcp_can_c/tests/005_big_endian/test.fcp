version: "3"

struct U8B {
    val @0: u8,
}

struct U16B {
    val @0: u16,
}

struct U32B {
    val @0: u32,
}

struct U64B {
    val @0: u64,
}

struct I8B {
    val @0: i8,
}

struct I16B {
    val @0: i16,
}

struct I32B {
    val @0: i32,
}

struct I64B {
    val @0: i64,
}

struct F32B {
    val @0: f32,
}

struct F64B {
    val @0: f64,
}

device ecu {
    protocol can {
        impl U8B {
            id: 11;
            signal val {
                endianness: "big",
            };
        }

        impl U16B {
            id: 12;
            signal val {
                endianness: "big",
            };
        }

        impl U32B {
            id: 13;
            signal val {
                endianness: "big",
            };
        }

        impl U64B {
            id: 14;
            signal val {
                endianness: "big",
            };
        }

        impl I8B {
            id: 15;
            signal val {
                endianness: "big",
            };
        }

        impl I16B {
            id: 16;
            signal val {
                endianness: "big",
            };
        }

        impl I32B {
            id: 17;
            signal val {
                endianness: "big",
            };
        }

        impl I64B {
            id: 18;
            signal val {
                endianness: "big",
            };
        }

        impl F32B {
            id: 19;
            signal val {
                endianness: "big",
            };
        }

        impl F64B {
            id: 20;
            signal val {
                endianness: "big",
            };
        }
    }
}
