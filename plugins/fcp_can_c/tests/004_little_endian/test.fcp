version: "3"

struct U8L {
    val @0: u8,
}

struct U16L {
    val @0: u16,
}

struct U32L {
    val @0: u32,
}

struct U64L {
    val @0: u64,
}

struct I8L {
    val @0: i8,
}

struct I16L {
    val @0: i16,
}

struct I32L {
    val @0: i32,
}

struct I64L {
    val @0: i64,
}

struct F32L {
    val @0: f32,
}

struct F64L {
    val @0: f64,
}

device ecu {
    protocol can {
        impl U8L { id: 1, }
        impl U16L { id: 2, }
        impl U32L { id: 3, }
        impl U64L { id: 4, }
        impl I8L { id: 5, }
        impl I16L { id: 6, }
        impl I32L { id: 7, }
        impl I64L { id: 8, }
        impl F32L { id: 9, }
        impl F64L { id: 10, }
    }
}
