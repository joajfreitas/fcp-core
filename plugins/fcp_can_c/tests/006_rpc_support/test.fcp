version: "3"

struct Msg {
    s1 @ 0: u8,
    s2 @ 1: u16,
}

impl can for Msg {
    id: 10,
    device: "ecu",
}

service SensorService @0 {
    method RequestState(SensorReq) @0 returns SensorInformation,
    method GetTemperature(SensorReq) @1 returns Temperature,
}

device ecu {
    rpc_get_id: 1025,
    rpc_ans_id: 1026,
    services: [SensorService],
}
