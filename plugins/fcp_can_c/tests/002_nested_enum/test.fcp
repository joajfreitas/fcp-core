version: "3"

enum InnerEnum {
    A = 0,
    B = 1,
    C = 2,
}

struct InnerStruct {
    s1 @0: u8,
    s2 @1: u16,
}

struct SensorInformation {
    e @0: InnerEnum,
    s @1: InnerStruct,
}

struct OuterStruct {
    inner @0: InnerStruct,
    val @1: u16,
}

struct MultiLevelStruct {
    x @0: InnerStruct,
    y @1: OuterStruct,
}

device ecu {
    protocol can {
        impl SensorInformation { id: 10, },
        impl OuterStruct { id: 20, },
        impl MultiLevelStruct { id: 30, },
    },
}
