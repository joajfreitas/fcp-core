version: "3"

enum InnerEnum {
    A = 0,
    B = 1,
    C = 2,
}

struct Level1Struct {
    s1 @0: u8,
    s2 @1: u8,
}

struct Level2Struct {
    l1 @0: Level1Struct,
    val2 @1: u8,
}

struct Level3Struct {
    l2 @0: Level2Struct,
    val3 @1: u8,
}

struct Level4Struct {
    l3 @0: Level3Struct,
    val4 @1: u8,
}

struct Level5Struct {
    l4 @0: Level4Struct,
    e @1: InnerEnum,
    val5 @2: u8,
}

struct SensorInformation {
    e @0: InnerEnum,
    l1 @1: Level1Struct,
}

device ecu {
    protocol can {
        impl SensorInformation { id: 10, },
        impl Level3Struct { id: 20, },
        impl Level5Struct { id: 30, },
    },
}