version: "3"

struct A {
    field1 @0: u2 | unit("m/s"),
    field2 @1: u8 | unit("m^2/s"),
}

struct B {
    field1 @0: u32,
    field2 @1: A,
}

device ecu1 {
    protocol can {
        impl A {
            id: 10,
            device: "ecu1",

            signal field2 {
                bitstart: 16,
            },
        },

        impl B {
            id: 11,
            device: "ecu1",
        },
    },
}
