{
  "name": "fcp",
  "displayName": "fcp",
  "description": "fcp language support for vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "fcp",
      "aliases": ["fcp", "fcp"],
      "extensions": [".fcp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "fcp",
      "scopeName": "source.fcp",
      "path": "./syntaxes/fcp.tmLanguage.json"
    }]
  }
}
